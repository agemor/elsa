class Student {
	var name:string = "Hyunjun";
	var mentor:Student;
	
	function printMyName():void{
		print(name);
	}
} 

function nameMixer(student:Student, student2:Student):string{
	return student.name + student2.name;
}

var boy:Student;
var girl:Student = new Student;

boy = new Student;

boy.name = "Daniel";
girl.name = "Olivia";

boy.mentor = girl;
print(boy.mentor.name);

print(nameMixer(boy,girl));

boy.printMyName();
print(boy.name);

girl.printMyName();
print(girl.name);