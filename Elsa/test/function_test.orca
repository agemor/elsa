/**
* 리턴하지 않는 파라미터가 없는 함수
*/
function printHelloWorld():void {
	print("Hello World!");
}
printHelloWorld();

/**
* 리턴하지 않는 파라미터가 있는 함수
*/
function printFullName(firstName:string, lastName:string):void {
	print(firstName + " "+ lastName);
}
printFullName("HyunJun", "Kim");

/**
* 리턴하는 파라미터가 없는 함수
*/
function getTau():number {
	return getPI() * 2;
}
function getPI():number {
	return 3.141592;
}
function getMyName():string {
	return "HyunJun Kim";
}
print(getPI());
print(getTau());
print(getMyName());

/**
* 리턴하는 파리미터가 있는 함수
*/
function average(n1:number, n2:number):number {
	return (n1 + n2) / 2;
}
print(average(average(average(100,300),300),150));